char Path[80];
char * get_path(char *path)
{	
		getcwd(Path, RT_CONSOLEBUF_SIZE - 8);	
		strcat(Path, "/");
		if (*path == '/')
			strcpy(Path, path);
		else
			strcat(Path, path);
//rt_kprintf("path = %s\n", Path);
		return Path;
}

